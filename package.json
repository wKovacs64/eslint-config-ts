{
  "name": "@wkovacs64/eslint-config-ts",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 ESLint config (TypeScript)",
  "keywords": [
    "eslint",
    "eslintconfig",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/eslint-config-ts.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/eslint-config-ts/issues"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts\" .",
    "validate": "yarn lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^1.11.0",
    "@wkovacs64/eslint-config": "^5.0.0",
    "eslint-config-prettier": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.1",
    "husky": "2.7.0",
    "lint-staged": "9.0.2",
    "prettier": "1.18.2",
    "semantic-release": "15.13.18",
    "typescript": "3.5.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "typescript": "^3.3.1"
  }
}
